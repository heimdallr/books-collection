{
	"outputFileName": "index.html",
	"outputFileExtension": "htm",
	"head": [
		"<!DOCTYPE html>",
		"<html lang=\"#LANGUAGE#\">",
		"<head>",
		"\t<title>#TITLE#</title>",
		"\t<meta charset=\"UTF-8\">",
		"\t<link rel=\"icon\" type=\"image/x-icon\" href=\"../img/FLibrary.ico\">",
		"\t<link id=\"theme\" rel=\"stylesheet\" href=\"../css/style2.css\">",
		"</head>",
		"<body>",
		"<div id=\"content\">",
		"<h1 id=\"header\">#HEAD#</h1>"
	],
	"tail": [
		"<h2 id=\"footer\">",
		"<span id=\"langbutton\">",
		"\t<button id=\"langbutton1\" onclick=\"document.location='../#OTHERLANGUAGE1#/index.html'\" style=\"background-image: url(../css/#OTHERLANGUAGE1#.png);\"></button>",
		"\t<button id=\"langbutton2\" onclick=\"document.location='../#OTHERLANGUAGE2#/index.html'\" style=\"background-image: url(../css/#OTHERLANGUAGE2#.png);\"></button>",
		"</span>",
		"#TAIL#",
		"<span id=\"button\"><button id=\"styles\" onclick=\"toggleTheme()\">#STYLE#</button>",
		"\t<script>",
		"\t\tfunction toggleTheme() {",
		"\t\t\tlet theme = document.getElementById('theme');",
		"\t\t\tif (theme.getAttribute('href') == '../css/style2.css') {",
		"\t\t\t\ttheme.setAttribute('href', '../css/style1.css');",
		"\t\t\t} ",
		"\t\t\telse {",
		"\t\t\t\ttheme.setAttribute('href', '../css/style2.css');",
		"\t\t\t}",
		"\t\t}",
		"\t</script></span>",
		"</h2>",
		"</div>",
		"</body>",
		"</html>"
	],
	"question": [
		{
			"before": [
				"<details>",
				"\t<summary> &#128211; #QUESTION#</summary>\n"
			],
			"after": "<hr>\n</details>\n\n"
		},
		{
			"before": [
				"<details>",
				"\t<summary class=\"subtitle\">#QUESTION#</summary>\n"
			],
			"after": "</details>\n"
		},
		{
			"before": [
				"<details>",
				"\t<summary class=\"subtitle\">#QUESTION#</summary>\n"
			],
			"after": "</details>\n"
		},
		{
			"before": [
				"<details>",
				"\t<summary class=\"sub_subtitle\">#QUESTION#</summary>\n"
			],
			"after": "</details>\n"
		}
	],
	"answer": {
		"simple": "<br>#ANSWER#\n",
		"tagged": "#ANSWER#\n",
		"tags": {
			"ul": "<ul>\n",
			"ul top10": "<ul class=\"top10\">\n",
			"ul nd": "<ul class=\"none_dot\">\n",
			"/ul": "</ul>\n",
			"ol": "<ol>\n",
			"ol top10": "<ol class=\"top10\">\n",
			"/ol": "</ol>\n",
			"hr": "<hr>\n",
			"hr lite": "<hr class=\"lite\">\n",
			"p": "<p>#ANSWER#\n",
			"p null": "<p class=\"null\">#ANSWER#\n",
			"p code": "<p class=\"code\">#ANSWER#\n",
			"p note": "<p class=\"note\">#ANSWER#\n",
			"/p": "</p>\n",
			"p s": "<p class=\"subtitle\">#ANSWER#</p>\n",
			"p ss": "<p class=\"sub_subtitle\">#ANSWER#</p>\n",
			"*": "<li>#ANSWER#</li>\n"
		}
	}
}
